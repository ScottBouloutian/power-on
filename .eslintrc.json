{
    "extends": "airbnb",
    "plugins": [
        "jest"
    ],
    "env": {
        "jest/globals": true
    },
    "rules": {
        "indent": ["error", 4],
        "react/jsx-indent": ["error", 4],
        "import/no-extraneous-dependencies": ["error", {"devDependencies": ["**/*.test.{js,jsx}"]}],
        "react/jsx-filename-extension": ["error", { "extensions": [".js"] }],
        "import/extensions": "off"
    }
}
